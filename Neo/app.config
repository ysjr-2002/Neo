<?xml version="1.0" encoding="utf-8"?>
<configuration>
  <appSettings>
    <!--通道号-->
    <add key="termId" value="2"/>
    <!--虚拟串口 如果没有，值为none-->
    <add key="virtualPort" value="None"/>
    <!--继电器串口-->
    <add key="switchPort" value="COM1"/>
    <!--WebURI-->
    <!--http://szneo.lvgemgroup.com.cn/index.php?g=Api&m=Face&a=checkCode2d-->
    <!--v.wapwei.com-->
    <add key="opendoorAPIUrl" value="http://szneo.lvgemgroup.com.cn/index.php?g=Api&amp;m=Face&amp;a=checkCode2d"/>
    <!--重启日 周日到周六  1:重启 0:不重启 -->
    <add key="rebootweekofDay" value="0,0,0,0,0,0,0"/>
    <!--重启时间-->
    <add key="rebootTime" value="00:00"/>
  </appSettings>
  <startup>
    <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.5"/>
  </startup>
</configuration>
