<?xml version="1.0" encoding="utf-8"?>
<configuration>
  <appSettings>
    <!--通道号-->
    <add key="termId" value="2"/>
    <!--虚拟串口 如果没有，值为none-->
    <add key="virtualPort" value="none"/>
    <!--继电器串口-->
    <add key="switchPort" value="COM1"/>
    <!--WebURI-->
    <add key="opendoorAPIUrl" value="https://meeting.yowhale.com/openapi/guard/openDoor.json"/>
    <!--重启周日-->
    <add key="rebootweekofDay" value="0,0,0,0,0,0,0"/>
    <!--重启时间-->
    <add key="rebootTime" value="00:00"/>
  </appSettings>
<startup><supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.5"/></startup></configuration>
